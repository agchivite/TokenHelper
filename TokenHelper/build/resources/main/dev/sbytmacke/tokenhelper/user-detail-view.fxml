<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="200.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            fx:controller="dev.sbytmacke.tokenhelper.controllers.UserDetailController">
	<VBox alignment="CENTER" spacing="20.0">
		<Label fx:id="usernameLabel" text="Username" underline="true">
			<padding>
				<Insets left="10.0" right="10.0" top="10.0"/>
			</padding>
			<font>
				<Font size="16.0"/>
			</font>
		</Label>
		<GridPane fx:id="gridPane" alignment="CENTER" prefHeight="92.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="195.0" minWidth="10.0" prefWidth="193.0"/>
				<ColumnConstraints hgrow="SOMETIMES"/>
			</columnConstraints>
			<VBox.margin>
				<Insets left="10.0" right="10.0"/>
			</VBox.margin>
		</GridPane>
		<Button onAction="#closeWindow" text="Close">
			<font>
				<Font size="13.0"/>
			</font>
			<VBox.margin>
				<Insets bottom="20.0"/>
			</VBox.margin>
		</Button>
	</VBox>
</AnchorPane>
